{
  "kind": "collectionType",
  "collectionName": "receipt_details",
  "info": {
    "singularName": "receipt-detail",
    "pluralName": "receipt-details",
    "displayName": "ReceiptDetail",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "product": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::product.product"
    },
    "unitPrice": {
      "type": "biginteger",
      "required": true
    },
    "quanity": {
      "type": "integer",
      "required": true,
      "default": 1
    },
    "receipt": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::receipt.receipt",
      "inversedBy": "receipt_details"
    }
  }
}
